local getgenv, getnamecallmethod, hookmetamethod, newcclosure, checkcaller, stringlower = getgenv, getnamecallmethod, hookmetamethod, newcclosure, checkcaller, string.lower
if getgenv().AntiKick then
	return
end
local Players, StarterGui, OldNamecall = game:GetService("Players"), game:GetService("StarterGui")
getgenv().AntiKick = {
	Enabled = true,
	SendNotifications = true,
	CheckCaller = false
}
OldNamecall = hookmetamethod(game, "__namecall", newcclosure(function(...)
	if (getgenv().AntiKick.CheckCaller and not checkcaller() or true) and stringlower(getnamecallmethod()) == "kick" and AntiKick.Enabled then
		if getgenv().AntiKick.SendNotifications then
			StarterGui:SetCore("SendNotification", {
				Title = "Menu",
				Text = "Anti Kick triggered",
				Icon = "rbxassetid://6238540373",
				Duration = 2,
			})
		end

		return nil
	end

	return OldNamecall(...)
end))
if getgenv().AntiKick.SendNotifications then
	StarterGui:SetCore("SendNotification", {
		Title = "Menu",
		Text = "Anti-Kick script loaded!",
		Icon = "rbxassetid://6238537240",
		Duration = 3,
	})
end
local OldNamecall = hookmetamethod(game.ReplicatedStorage.Interaction.Ban, "__namecall", newcclosure(function(...)
    if (getgenv().AntiKick.CheckCaller and not checkcaller() or true) and stringlower(getnamecallmethod()) == "ban" and AntiKick.Enabled then
        if getgenv().AntiKick.SendNotifications then
            StarterGui:SetCore("SendNotification", {
                Title = "Menu",
                Text = "AntiBan Triggered",
                Icon = "rbxassetid://6238540373",
                Duration = 2,
            })
        end

        return nil
    end

    return OldNamecall(...)
end))

print("Loaded AntiBan And AntiKick")
